<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <Button fx:id="enc" layoutX="292.0" layoutY="67.0" mnemonicParsing="false" onAction="#encode" text="Encode" textFill="#222020">
         <font>
            <Font size="18.0" />
         </font></Button>
      <TextField fx:id="txtenc" layoutX="14.0" layoutY="67.0" promptText="Enter value to Encode">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <TextField fx:id="txtdec" layoutX="14.0" layoutY="212.0" promptText="Enter value to Decode">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <Button fx:id="dec" layoutX="291.0" layoutY="212.0" mnemonicParsing="false" onAction="#decode" text="Decode">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Button fx:id="exe" layoutX="186.0" layoutY="339.0" mnemonicParsing="false" onAction="#close" prefHeight="28.0" prefWidth="52.0" text="Exit">
         <font>
            <Font size="18.0" />
         </font></Button>
      <TextField fx:id="encodedtxt" editable="false" layoutX="14.0" layoutY="120.0" promptText="Encoded value">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="decodedtxt" editable="false" layoutX="14.0" layoutY="263.0" promptText="Decoded value">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
